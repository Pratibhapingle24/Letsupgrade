      "source": [
        "num1 = 1042000\n",
        "num2 = 702648265\n",
        "\n",
        "for num in range(num1, num2 + 1):\n",
        "    order = len(str(num))\n",
        "    total = 0\n",
        "    temp = num\n",
        "    while temp > 0:\n",
        "        digit = temp % 10\n",
        "        total += digit ** order\n",
        "        temp //= 10\n",
        "\n",
        "    if num == total:\n",
        "        print(num)\n",
        "        break"
      ],
